# KINAMBA ANPR Edge Service Configuration

# ERM API endpoint
ERM_API_URL=http://localhost:4000

# API authentication key (must match server's EDGE_API_KEY)
ERM_API_KEY=your-secure-api-key-here

# Camera configuration
CAMERA_ID=CAM1
RTSP_URL=rtsp://192.168.1.100:554/stream

# Plate detection confidence threshold (0-100)
CONFIDENCE_THRESHOLD=85

# Optional: OpenALPR config path (if using OpenALPR)
OPENALPR_CONFIG=/etc/openalpr/openalpr.conf
OPENALPR_RUNTIME=/etc/openalpr/runtime_data
